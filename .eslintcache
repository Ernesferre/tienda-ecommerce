        \r\n        }\r\n           \r\n      }\r\n\r\n\r\n\r\n    \r\n    return (\r\n        \r\n        \r\n        \r\n        <article className=\"product\">\r\n            \r\n            <div className=\"foto\">\r\n                <img src={`/products/${item.imagen}`} alt=\"\" />\r\n            </div>\r\n            \r\n            <div className=\"info container mt-3 text-center\">\r\n                <h1 className=\"title\">{item.titulo}</h1>\r\n                    {\r\n                        !!item.description && <p className=\"description\">{item.descriction}</p>\r\n                    }\r\n                <p className=\"descripcion\"> {item.descriction}</p>\r\n                <p className=\"price\">${item.precio}</p>\r\n                \r\n                <div className=\"d-flex justify-content-center\">\r\n\r\n                    <button className= \"btn btn-secondary\" onClick={decrementar} disabled={qty <= 1 ? true : null }>  - </button>\r\n                    <h2 className=\"hola mt-1 strong mt-4 ml-3 mr-3\">{qty}</h2>\r\n                    <button className=\"btn btn-secondary\" onClick={incrementar}  disabled= {qty === item.stock ? true : null }> + </button>\t\r\n                    \r\n\r\n                </div>\r\n                \r\n                <button className=\"agregar\" className=\"btn-lg mt-5\" onClick={onAdd}>Agregar al carrito</button>\r\n            \r\n            </div>\r\n        </article>\r\n    )\r\n}\r\n\r\nexport default ProductDetail;\r\n\r\n",["200","201"],"C:\\Users\\ferre\\Desktop\\react\\mitienda\\src\\components\\Assets\\productos.jsx",[],"C:\\Users\\ferre\\Desktop\\react\\mitienda\\src\\components\\general\\Nav\\Nav.jsx",[],"C:\\Users\\ferre\\Desktop\\react\\mitienda\\src\\components\\Checkout\\index.jsx",["202"],"C:\\Users\\ferre\\Desktop\\react\\mitienda\\src\\components\\general\\WidgetCart\\WidgetCart.jsx",["203"],"\r\nimport {useContext} from 'react';\r\nimport './WidgetCart.css';\r\nimport {Store} from '../../..